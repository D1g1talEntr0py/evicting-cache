{
  "name": "evicting-cache",
  "author": "D1g1talEntr0py",
  "version": "2.2.1",
  "license": "ISC",
  "description": "Cache implementation with an LRU evicting policy",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/evicting-cache.d.ts",
      "import": "./src/evicting-cache.js"
    }
  },
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "tsbuild",
    "build:watch": "tsbuild --watch",
    "type-check": "tsbuild --typeCheck",
    "lint": "eslint",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "prepublish": "pnpm lint && pnpm test && pnpm -s build"
  },
  "keywords": [
    "cache",
    "evicting-cache",
    "lru",
    "lru-cache",
    "evicting-lru-cache"
  ],
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.37.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.7.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-jsdoc": "^61.1.0",
    "globals": "^16.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vitest": "^3.2.4"
  },
  "browserslist": [
    "defaults",
    "not ios_saf < 15",
    "not op_mini all"
  ]
}
